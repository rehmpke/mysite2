{
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": 2,
        "no-unused-vars": [ 2 ],
        "no-dupe-keys": [ 2 ],
        "no-undefined": [ 0 ],
        "prefer-const": [ 2 ],
        "no-undef": [ 2 ]
    }
}